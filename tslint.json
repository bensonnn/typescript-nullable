{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "curly": [ true, "ignore-same-line" ],
    "only-arrow-functions": false,
    "quotemark": [ true, "single" ],
    "semicolon": [ true, "never" ],
    "space-before-function-paren": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-trailing-underscore",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}
